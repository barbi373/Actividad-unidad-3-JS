var numeroa = 10//(3)
var numerob = 15

function sumarDosNumeros(x){//(4)
    var sumar = x
    return sumar //(1)
}
sumarDosNumeros(numeroa)//(2)
//entonces va a abajo, va a numeroa, reemplaza

