var numeroa = 10//(3)
var numerob = 15

function sumarDosNumeros(x){//(4)
    var sumar = x
    console.log('tirando liea de funcion con retorno')
    return sumar //(1)
}
var guardarValorFuncion = sumarDosNumeros(numeroa)//(2)
console.log(guardarValorFuncion)
//cdo trabajamos con una funcion queremos
//que almacene algo en el return